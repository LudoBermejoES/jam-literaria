{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "ts-node --transpile-only index.ts",
    "start:prod": "npm run build && node dist/index.js",
    "dev": "nodemon --exec ts-node --transpile-only index.ts",
    "test": "jest --selectProjects=server",
    "test:nocov": "jest --no-coverage",
    "test:js": "NODE_OPTIONS=--unhandled-rejections=strict jest --testMatch='**/*.test.js' --no-coverage",
    "build": "tsc && npm run copy-prisma",
    "copy-prisma": "node -e \"require('fs-extra').copySync('prisma', 'dist/prisma')\" && node -e \"require('fs-extra').copySync('generated', 'dist/generated')\"",
    "test:watch": "jest --watch",
    "generate-prisma": "prisma generate",
    "prisma:seed": "ts-node --transpile-only prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "prisma": "^6.7.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^5.0.1",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "fs-extra": "^11.3.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.0.0",
    "nodemon": "^3.1.1",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "description": "",
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  }
}
